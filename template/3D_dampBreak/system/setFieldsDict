FoamFile
{
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
    boxToCell
    {
        box (1.992 -0.5 0) (3.22 0.5 0.55);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);

